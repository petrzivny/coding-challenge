{% extends 'base.html.twig' %}

{% block title %}{{ 'new.post'|trans|capitalize }}{% endblock %}

{% block body %}
    {{ form_start(form) }}
        {{ form_widget(form) }}
        <input type="submit" value="{{ 'create'|trans|capitalize }}"/>
        <a href="javascript:history.back()">{{ 'cancel'|trans|capitalize }}</a>
    {{ form_end(form) }}
{% endblock %}